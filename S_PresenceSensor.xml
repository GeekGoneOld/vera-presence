<?xml version="1.0" encoding="utf-8"?>
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>5</minor>
	</specVersion>
	<actionList>
		<action>
			<name>GetDeviceType</name>
			<argumentList>
				<argument>
					<name>DeviceType</name>
					<direction>out</direction>
					<relatedStateVariable>DeviceType</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetDeviceType</name>
			<argumentList>
				<argument>
					<name>newDeviceTypeValue</name>
					<direction>in</direction>
					<relatedStateVariable>DeviceType</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetAddress</name>
			<argumentList>
				<argument>
					<name>AddressValue</name>
					<direction>out</direction>
					<relatedStateVariable>Address</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetAddress</name>
			<argumentList>
				<argument>
					<name>newAddressValue</name>
					<direction>in</direction>
					<relatedStateVariable>Address</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetPresent</name>
			<argumentList>
				<argument>
					<name>newPresentValue</name>
					<direction>in</direction>
					<relatedStateVariable>Present</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
	<serviceStateTable>
		<stateVariable>
			 <sendEventsAttribute>no</sendEventsAttribute>
			 <name>Address</name>
			 <dataType>string</dataType>
			 <defaultValue>Enter an MAC Address (e.g. 00:00:00:00:00:00)</defaultValue>
		</stateVariable>
		<stateVariable>
			 <sendEventsAttribute>no</sendEventsAttribute>
			 <name>DeviceType</name>
			 <dataType>string</dataType>
			 <defaultValue>bluetooth</defaultValue>
		</stateVariable>
		<stateVariable>
			 <sendEventsAttribute>no</sendEventsAttribute>
			 <name>Present</name>
			 <dataType>string</dataType>
			 <defaultValue>no</defaultValue>
		</stateVariable>
	</serviceStateTable>
</scpd>